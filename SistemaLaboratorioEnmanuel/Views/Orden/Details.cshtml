@model List<LaboratorioEnmanuel.Models.sp_ExamenDetalleOrdenes_Result>
@{
    ViewBag.Title = "Detalle De Orden";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

<div class="row justify-content-center">
    <div class="col-2">
        <div>
            <label>Id</label>

        </div>
    </div>
    <div class="col-5">
        <label>Examen</label>
    </div>
    <div class="col-2">
        <label>Acciones</label>
    </div>

</div>
@foreach (var item in Model)
{
    <div class="row justify-content-center">
        <div class="col-2">
            <div>
                @item.Id
            </div>
        </div>
        <div class="col-5">
            @item.NombreTipoExamen
        </div>
        <div class="col-2">
            <input type="button" class="btn btn-primary" value="AñadirResultado"/>           
            @Html.ActionLink("Añadir Resultado", "ResultadoExamen", new {id = item.Id }, new { @class = "btn btn-primary" })
        </div>
    </div>
}
